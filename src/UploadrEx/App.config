<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <log4net configSource="log4net.config" />
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="RollingLogFileAppender_Diagnostics" type="System.Diagnostics.TextWriterTraceListener" initializeData="${APPDATA}\logs.log" />
      </listeners>
    </trace>
  </system.diagnostics>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="log4net.Internal.Debug" value="true" />
  </appSettings>
</configuration>